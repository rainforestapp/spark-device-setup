{
  "name": "device-setup",
  "version": "1.0.0",
  "description": "Setup Particle Devices via Browser",
  "main": "src/DeviceSetup.js",
  "scripts": {
    "prepublish": "npm run lint && npm run compile",
    "compile": "./node_modules/.bin/babel --optional runtime -sd lib/ src/",
    "test": "./node_modules/.bin/mocha test/ -R spec --compilers js:babel/register",
    "cover": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha test/ -- -R spec --compilers js:babel/register",
    "bundle": "./node_modules/.bin/browserify ./src -t babelify -o ./public/js/DeviceSetup.js",
    "lint": "./node_modules/.bin/eslint src/"
  },
  "author": "Emily Rose <nexxy@particle.io>",
  "license": "LGPL-3.0",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "eslint": "^1.5.1",
    "eslint-config-airbnb": "0.0.8",
    "eslint-plugin-react": "^3.4.2",
    "gulp": "^3.9.0",
    "gulp-connect": "^2.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.4.1",
    "istanbul": "^0.3.21",
    "mocha": "^2.3.3",
    "should": "^7.1.0",
    "sinon": "^1.17.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.4.0"
  },
  "dependencies": {
    "babel-runtime": "^5.8.24",
    "jquery": "^2.1.4",
    "react": "^0.13.3",
    "softap-setup": "^3.0.1"
  }
}
